{
  "system": {
    "key": "thisisakey",
    "name": "Byreader"
  },
  "db": {
    "uri": "mongodb://root:root@127.0.0.1:27017/",
    "name": "byreader"
  },
  "queue": {
    "comic": {
      "concurrency": 15,
      "timeout": 5000
    }
  },
  "cache": {
    "NodeCache": {
      "stdTTL": 60
    },
    "type": "NodeCache"
  },
  "fileCache": {
    "enable": true,
    "maxSize": 1024,
    "maxFileSize": 10,
    "path": "cache"
  },
  "koa": {
    "port": 3000
  },
  "imageCompress": {
    "jpeg": {
      "quality": 80
    },
    "webp": {
      "quality": 80
    },
    "avif": {
      "quality": 80
    }
  }
}